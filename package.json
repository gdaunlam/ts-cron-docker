  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build api",
    "build:command": "nest build command",
    "build:consumer": "nest build consumer",
    "command": "CLI_PATH=dist/apps/command/main.js npx nestjs-command",
    "command:local": "cross-env SEED_ENV=local CLI_PATH=dist/apps/command/main.js npx nestjs-command",
    "command:test": "cross-env SEED_ENV=test CLI_PATH=dist/apps/command/main.js npx nestjs-command",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": " nest start api",
    "start:local": "cross-env SEED_ENV=local nest start api --debug 0.0.0.0:9662 --watch",
    "start:prod": "node dist/apps/api/main.js",
    "start:consumer": "SEED_ENV=local nest start consumer",
    "start:local:consumer": "SEED_ENV=local nest start consumer --watch",
    "start:prod:consumer": "node dist/apps/consumer/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/project-name/test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:local": "SEED_ENV=local node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
